<plugin>
	<extension point="org.eclipse.ui.popupMenus">
   
		<objectContribution adaptable="true" id="genpom.popupmenu" objectClass="org.eclipse.core.resources.IProject">
 			<filter name="projectNature" value="com.tibco.zion.dc.ApplicationNature"/>
 			<action class="com.tibco.bw.studio.maven.action.MavenPOMProcessor" id="bw6.genpom" label="Generate POM for Application" menubarPath="additions"/>
  		</objectContribution>
		<objectContribution adaptable="true" id="genmf.popupmenu" objectClass="org.eclipse.core.resources.IProject">
 			<filter name="projectNature" value="com.tibco.zion.dc.ApplicationNature"/>
 			<action class="com.tibco.bw.studio.maven.action.ManifestProcessor" id="bw6.genmf" label="Update Manifest with POM Dependencies" menubarPath="additions"/>
  		</objectContribution>   
   </extension>
 <extension
       point="com.tibco.bw.debug.launch">
    <Name
          class="com.tibco.bw.studio.maven.extension.MavenDebugLauncher">
    </Name>
